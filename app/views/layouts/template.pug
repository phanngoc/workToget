<!DOCTYPE html>
html
  head
    title #{title}
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    block styles
      link(href=baseUrl + '/css/font-awesome.min.css', rel='stylesheet', type='text/css')
      link(href=baseUrl + '/css/bootstrap.css', rel='stylesheet', type='text/css')
      link(href=baseUrl + '/css/app.css', rel='stylesheet', type='text/css')
      link(href=baseUrl + '/css/home.css', rel='stylesheet', type='text/css')
      link(href=baseUrl + '/css/trix.css', rel='stylesheet', type='text/css')
      link(href=baseUrl + '/css/fullcalendar.min.css', rel='stylesheet', type='text/css')

    meta(name="baseUrl", content=baseUrl)
  body
    block content

    block scripts
      script(src=baseUrl + '/js/jquery.min.js')
      <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
      script(src=baseUrl + '/js/bootstrap.js')
      script(src=baseUrl + '/js/app.js')
      script(src=baseUrl + '/js/trix.js')
      script(src=baseUrl + '/js/moment.min.js')
      script(src=baseUrl + '/js/fullcalendar.min.js')
